  pages = {164-173},
  file = {/home/moorepants/Zotero/storage/ULJGGUC9/Eilenberg et al. - 2010 - Control of a Powered AnkleFoot Prosthesis Based o.pdf;/home/moorepants/Zotero/storage/RLY7YAHQ/5378631.html}
