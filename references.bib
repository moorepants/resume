  volume = {10},
  issn = {1742-5689, 1742-5662},
  shorttitle = {Walking, Running, and Resting under Time, Distance, and Average Speed Constraints},
  doi = {10.1098/rsif.2012.0980},
  abstract = {On a treadmill, humans switch from walking to running beyond a characteristic transition speed. Here, we study human choice between walking and running in a more ecological (non-treadmill) setting. We asked subjects to travel a given distance overground in a given allowed time duration. During this task, the subjects carried, and could look at, a stopwatch that counted down to zero. As expected, if the total time available were large, humans walk the whole distance. If the time available were small, humans mostly run. For an intermediate total time, humans often use a mixture of walking at a slow speed and running at a higher speed. With analytical and computational optimization, we show that using a walk\textendash{}run mixture at intermediate speeds and a walk\textendash{}rest mixture at the lowest average speeds is predicted by metabolic energy minimization, even with costs for transients\textemdash{}a consequence of non-convex energy curves. Thus, sometimes, steady locomotion may not be energy optimal, and not preferred, even in the absence of fatigue. Assuming similar non-convex energy curves, we conjecture that similar walk\textendash{}run mixtures may be energetically beneficial to children following a parent and animals on long leashes. Humans and other animals might also benefit energetically from alternating between moving forward and standing still on a slow and sufficiently long treadmill.},
  language = {en},
  number = {81},
  journal = {Journal of The Royal Society Interface},
  author = {Long, Leroy L. and Srinivasan, Manoj},
  month = jun,
  year = {2013},
  keywords = {Optimization,energy minimization,gait transition,legged locomotion,walking and running},
  file = {/home/moorepants/Zotero/storage/8M42REHA/Long and Srinivasan - 2013 - Walking, running, and resting under time, distance.pdf;/home/moorepants/Zotero/storage/5V29TVI6/20120980.html;/home/moorepants/Zotero/storage/SWNGFQCB/20120980.html},
  pmid = {23365192}
}

